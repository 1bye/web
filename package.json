{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:docs": "turbo run build --filter xyflow-docs",
    "build:platform": "turbo run build --filter xyflow-platform",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter xyflow-docs",
    "dev:platform": "turbo run dev --filter xyflow-platform",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "docs"
}
